<!--Include header-->
<%- include("include/_header") %>
<!--/Include header-->

<!--Main Site-->
<main id="site-main">
  <div class="container">
    <div class="box-nav d-flex-justify-between">
      <a href="/add-user" class="border-shadow">
        <span class="text-gradient"
          ><i class="fas fa-user"></i>New User</span
        ></a
      >
    </div>
    <!--Form Handling -->
    <form action="/" method="POST">
      <table class="table">
        <thead class="thead-dark">
          <tr>
            <th>ID :</th>
            <th>Name :</th>
            <th>@Email :</th>
            <th>Gender :</th>
            <th>Status :</th>
            <th>Action :</th>
          </tr>
        </thead>
        <tbody>
          <!--Include Show-->
          <%- include("include/_show") %>
          <!--/Include Show-->
        </tbody>
      </table>
    </form>
  </div>
</main>
<!--/Main Site-->
<!--Include footer-->
<%- include("include/_footer") %>
<!--/Include footer-->
